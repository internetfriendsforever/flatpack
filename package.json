    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "color-js": "^1.0.4",
    "express": "^4.14.1",
    "history": "^4.5.1",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-contenteditable": "^2.0.2",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.0",
    "redux-thunk": "^2.2.0",
    "transform-loader": "^0.2.3",
    "webpack": "^2.2.1",
